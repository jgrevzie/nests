<h1>Listing nurses</h1>

<table>
  <tr>
    <th>Username</th>
    <th>Admin</th>
    <th>Validator</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @nurses.each do |nurse| %>
  <tr>
    <td><%= nurse.username %></td>
    <td><%= nurse.admin %></td>
    <td><%= nurse.validator %></td>
    <td><%= link_to 'Show', nurse %></td>
    <td><%= link_to 'Edit', edit_nurse_path(nurse) %></td>
    <td><%= link_to 'Destroy', nurse, method: :delete, data: { confirm: 'Are you sure?' } %></td>
    <td><%= link_to 'Add Proc', add_procedure_nurse_path(nurse) %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Nurse', new_nurse_path %>
<%= button_to 'Logout', logout_path, method: :delete %>